{

  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://news.google.com/; object-src 'self'",
  "description": "An Google Now themed top news stories reader for your new tab page",
  "manifest_version": 2,
  "name": "News Tab",
  "permissions": [

    "https://news.google.com/",
    "storage"
  ],
  "short_name": "News Tab",
  "version": "0.0.1",
  "icons": { "16": "images/icon-16.png",
    "128": "images/icon-128.png" }
}
